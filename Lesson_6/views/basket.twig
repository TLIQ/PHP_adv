{% extends 'main.twig' %}

{% block title %}Корзина{% endblock %}

{% block content %}


    {% for good in goods %}
        <p>Товар: {{ good.name }}</p>
        <p>Цена: {{ good.price }}</p>
        <p>Количество: {{ good.count }}</p>
    {% else %}
        Корзина пуста
    {% endfor %}
{% endblock %}